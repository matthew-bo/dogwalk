    "@types/jsonwebtoken": "^9.0.1",
    "@types/uuid": "^9.0.1",
    "@types/node-cron": "^3.0.7",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.6",
    "tsc-alias": "^1.8.6",
    "prisma": "^4.13.0",
    "rimraf": "^5.0.0",

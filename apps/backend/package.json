    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "joi": "^17.9.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "@prisma/client": "^4.13.0",
    "redis": "^4.6.6",
    "ioredis": "^5.3.2",
    "socket.io": "^4.6.1",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0",
